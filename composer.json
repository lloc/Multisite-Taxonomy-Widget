{
  "name": "lloc/multisite-taxonomy-widget",
  "description": "Multisite Taxonomy Widget",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "homepage": "https://it.wordpress.org/plugins/multisite-taxonomy-widget/",
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.9.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7",
    "brain/monkey": "2.*"
  },
  "autoload": {
    "psr-4": {
      "lloc\\Mtw\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "lloc\\MtwTests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "coverage": "phpunit --coverage-html docs/coverage"
  },
  "authors": [
    {
      "name": "Dennis Ploetner",
      "email": "re@lloc.de",
      "homepage": "http://lloc.de"
    }
  ],
  "keywords": [
    "multisite",
    "taxonomy",
    "widget"
  ]
}
